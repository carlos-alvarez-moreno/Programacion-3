public class tablaTraductor {
  public static void main(String[] args) {
    System.out.printf("%-10s %s\n", "home", "casa");
    System.out.printf("%-10s %s\n", "dog", "perro");
    System.out.printf("%-10s %s\n", "plane", "avion");
    System.out.printf("%-10s %s\n", "flower", "flor");
    System.out.printf("%-10s %s\n", "dress", "vestido");
    System.out.printf("%-10s %s\n", "launch", "almuerzo");
    System.out.printf("%-10s %s\n", "sky", "cielo");
    }
  }
